{
  "name": "Blognone Bot Hider",
  "description": "Make your blognone experience better",
  "version": "2.0",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["jquery.min.js", "background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ "jquery.min.js", "background.js" ],
    "matches": [ "http://*.blognone.com/*", "https://*.blognone.com/*"]
  }]
}